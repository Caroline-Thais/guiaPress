<%- include ('partials/header.ejs'); %>
<%- include ('partials/homenavbar.ejs'); %>
<body>
<br>
<div class="container">

    <div class="jumbotron">
        <h1 class="display-4">Bem vindo(a) ao Guia Press!</h1>
        <p class="lead">Este é um blog sobre Programação.</p>
        <hr class="my-4">
        <p>Vários artigos interessates.</p>
        <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
      </div>

    <% articles.forEach(article => { %>
        <div class="card">
            <div class="card-header">
                <h2><%= article.title %></h2>
            <div class="card-body">
                <a href="/<%= article.slug %>" class="btn btn-success">Ler artigo</a>
            </div>
        </div>
   <% }) %>



</body>
<%- include ('partials/footer.ejs'); %>